def ByteCount(Num):
    assert Num >= 0, 'INPUT MUST BE POSITIVE'
    sum = 0
    for byte in bin(Num):
        if byte == '1':
            sum += 1

    return sum

def Test(Input, ref):
    ans = ByteCount(Input)
    print('Given number:', Input, ' ', 'Byte form: ', bin(Input))
    print('Positive bytes =', ref, '; ','Calculated positive bytes =', ans)
    assert ans == ref, 'TEST FAILED'
    print('---------')

